<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.plugspot.mapper.cSpeedCMapper">

  <select id="citySpeedCharger" parameterType="citySpeedChargerDTO" resultType="citySpeedChargerDTO">
		select a.loc_seq,b.do_city,a.fast_charge,a.slow_charge
		from T_SPEED_CITY_CHARGE a, t_do_city b
        where a.loc_seq=b.loc_seq
  </select>
  
  
  

  </mapper>